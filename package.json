{
  "name": "@uploadcare/api",
  "version": "1.0.0",
  "description": "JavaScript library for work with Uploadcare API",
  "main": "dist/uploadcare.umd.js",
  "module": "dist/uploadcare.esm.js",
  "scripts": {
    "build": "flow && rollup -c rollup.config.js",
    "lint": "eslint .",
    "release": "release-it",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "jest --no-cache src/",
    "test:node:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watchAll src/",
    "test:browser": "npm run test:browser:prepare && karma start karma.config.js --single-run --nocache",
    "test:browser:prepare": "node scripts/build-jest-matchers.js",
    "test:browser:debug": "npm run test:browser:prepare && karma start karma.config.js --browsers=Chrome --single-run=false --watch --debug  --nocache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-javascript.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-javascript/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-javascript#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/node": "^7.0.0-beta.47",
