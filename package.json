{
  "name": "@uploadcare/upload-api",
  "version": "1.0.0-alpha",
  "description": "JavaScript library for work with Uploadcare Upload API",
  "main": "dist/uploadcare.cjs.js",
  "module": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint .",
    "check": "flow check",
    "test": "npm-run-all lint check test:node test:browser",
    "test:node": "BABEL_ENV=test:jest jest --no-cache --runInBand test/",
    "test:node:debug": "BABEL_ENV=test:jest node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watchAll test/",
    "test:browser": "BABEL_ENV=test node scripts/build-karma-injectables.js && karma start karma.config.js --single-run --nocache",
    "test:browser:debug": "BABEL_ENV=test node scripts/build-karma-injectables.js && karma start karma.config.js --browsers=Chrome --single-run=false --watch --debug  --nocache",
    "prebuild": "npm run clean",
    "build": "rollup -c rollup.config.js",
    "prerelease": "npm run test",
    "release": "release-it"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-js-upload-api.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-js-upload-api/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-js-upload-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@uploadcare/release-it": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "data-uri-to-buffer": "^2.0.0",
    "dataurl-to-blob": "0.0.1",
    "eslint": "^5.7.0",
    "eslint-config-uploadcare": "^1.3.0",
    "eslint-plugin-flowtype": "^3.0.0",
    "flow-bin": "^0.83.0",
    "flow-typed": "^2.5.1",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jest": "^23.6.0",
    "jest-matchers": "^20.0.3",
    "jest-mock": "^23.2.0",
    "karma": "^3.0.0",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-preprocessor": "^6.0.1",
    "lodash": "^4.17.11",
    "npm-run-all": "^4.1.3",
    "puppeteer": "^1.9.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-license": "^0.7.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-node-resolve-magic": "^0.2.11",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-size-snapshot": "^0.7.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "form-data": "^2.3.2"
  }
}
