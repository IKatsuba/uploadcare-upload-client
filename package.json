    "eslint-plugin-flowtype": "^2.47.1",
    "flow-bin": "^0.73.0",
    "husky": "^0.14.3",
    "jasmine": "^2.99.0",
    "jasmine-core": "^2.99.1",
    "jest": "^22.4.4",
    "jest-matchers": "^20.0.3",
    "karma": "^2.0.2",
    "karma-browserstack-launcher": "^1.3.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-rollup-preprocessor": "^6.0.0",
