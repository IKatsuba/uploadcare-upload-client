{
  "name": "@uploadcare/upload-client",
  "version": "2.0.0-alpha.2",
  "description": "Library for work with Uploadcare Upload API",
  "type": "module",
  "main": "lib/index.js",
  "module": "module/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*",
    "module/**/*"
  ],
  "browser": {
    "./lib/request/request.node.js": "./lib/request/request.browser.js",
    "./lib/tools/getFormData.node.js": "./lib/tools/getFormData.browser.js",
    "./lib/tools/sockets.node.js": "./lib/tools/sockets.browser.js",
    "./module/request/request.node.js": "./module/request/request.browser.js",
    "./module/tools/getFormData.node.js": "./module/tools/getFormData.browser.js",
    "./module/tools/sockets.node.js": "./module/tools/sockets.browser.js"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": "./lib/index.js",
      "import": "./module/index.js"
    }
  },
  "scripts": {
    "check-env-vars": "node ./checkvars.js",
    "mock:start": "ts-node --project ./mock-server/tsconfig.mock.json ./mock-server/server.ts --silent",
    "clean": "rimraf lib",
    "lint": "eslint . --ext=ts",
    "test": "start-server-and-test mock:start :3000 test:jest",
    "test:production": "npm run check-env-vars && TEST_ENV=production jest --config=configs/jest.config.js",
    "test:jest": "jest --config=configs/jest.config.js",
    "prebuild": "npm run clean",
    "build:cjs": "tsc --build configs/tsconfig.cjs.json",
    "build:module": "ttsc --build configs/tsconfig.module.json",
    "build": "npm run build:cjs && npm run build:module",
    "release:prepare": "shipjs prepare",
    "release:trigger": "shipjs trigger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uploadcare/uploadcare-upload-client.git"
  },
  "author": "Uploadcare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uploadcare/uploadcare-upload-client/issues"
  },
  "homepage": "https://github.com/uploadcare/uploadcare-upload-client#readme",
  "keywords": [
    "uploadcare",
    "file",
    "uploader",
    "image",
    "upload",
    "store",
    "cdn"
  ],
  "devDependencies": {
    "@koa/cors": "3.1.0",
    "@koa/router": "8.0.8",
    "@magic-works/ttypescript-browser-like-import-transformer": "2.2.0",
    "@types/form-data": "2.5.0",
    "@types/jest": "26.0.5",
    "@types/koa": "2.11.3",
    "@types/node": "12.12.51",
    "@types/promise": "7.1.30",
    "@types/ws": "7.2.6",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "chalk": "4.1.0",
    "data-uri-to-buffer": "3.0.1",
    "dataurl-to-blob": "0.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.5.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "jest": "26.1.0",
    "jest-environment-jsdom": "26.1.0",
    "jest-websocket-mock": "2.1.0",
    "koa": "2.13.0",
    "koa-add-trailing-slashes": "2.0.1",
    "koa-body": "4.2.0",
    "mock-socket": "9.0.3",
    "prettier": "1.19.1",
    "prettier-config-standard": "1.0.1",
    "rimraf": "3.0.2",
    "shipjs": "0.20.1",
    "start-server-and-test": "1.11.2",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "ttypescript": "1.5.10",
    "typescript": "3.9.7"
  },
  "dependencies": {
    "form-data": "^3.0.0",
    "ws": "^7.2.1"
  }
}
